{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**",
                ".next/**",
                "!.next/cache/**"
            ]
        },
        "dev": {
            "persistent": true,
            "cache": false
        },
        "clean": {
            "cache": false
        },
        "publint": {
            "cache": false
        },
        "lint": {},
        "check": {}
    }
}